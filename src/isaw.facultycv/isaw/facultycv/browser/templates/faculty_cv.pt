    <!--
		<img width=200 class="image-right captioned" tal:attributes="src context/profile/image/absolute_url | context/++resource++cvimages/face-grin.png;
        image-right.border string:0" />
		<dl id="cv-toc" class="portlet toc" style="display: none"
    		i18n:domain="plone">
  			<dt class="portletHeader"
      		i18n:translate="label_tableofcontents">Contents</dt>
  			<dd class="portletItem">
			test
  			</dd>
		</dl>
    -->
		<p class="facultyTitles">Curriculum Vitae</p>
		<a tal:attributes="href string:${context/absolute_url}/pdf_view">Download PDF</a>

        <div class="facultyContact">
		    <div class="address" tal:content="structure context/aq_parent/Address" />
		    <a tal:condition="context/Email" tal:attributes="mailto string:${context/aq_parent/Email}" tal:content="string:${context/Email}" />
            <span class="phone" tal:content="structure context/aq_parent/Phone | nothing" />
            <div class="visualClear"><!-- --></div>
        </div>
    
        <table id="faculty-cv">

        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/Education and Degrees">Education and Degrees</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Education and Degrees | nothing" />

        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/Positions held">Positions Held</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Positions held | nothing" />
        </tr>

        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/Honors">Honors</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Honors | nothing" />
        </tr>

        <!--
        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/Professional Affiliations">Professional Affiliations</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Professional Affiliations | nothing" />
        </tr>
        -->

        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/Professional Offices and Service">Professional Offices and Service</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Professional Offices and Service | nothing" />
        </tr> 


        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/Research and works in progress">Research and Work in progress</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Research and works in progress | nothing" />
        </tr>


        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/Research Awards">Research Awards</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Research Awards | nothing" />
        </tr>

        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/List of Courses">List of Courses</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Courses Taught | nothing "/>
        </tr>

        
        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/Graduate Supervision">Graduate Supervision</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Graduate Supervision | nothing "/>
        </tr>


        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/Publications">Publications</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Publications | nothing "/>
        </tr>


        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/Papers">Papers</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Papers | nothing "/>
        </tr>

        <tr>
            <th tal:attributes="colspan string:2" tal:condition="context/Lectures">Lectures</th>
        </tr>
        <tr>
            <td tal:attributes="colspan string:2" tal:content="structure context/Lectures | nothing" />
        </tr>

    </table>
    </metal:content-core>
</metal:content-core>

</body>
</html>

