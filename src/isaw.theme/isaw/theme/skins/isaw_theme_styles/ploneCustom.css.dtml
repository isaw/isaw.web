/*
  <dtml-with base_properties> (do not remove this)
  <dtml-call "REQUEST.set('portal_url', portal_url())"> (not this either)
*/

body {
    background-color: #6f5c54;
    *padding-top: 25px;
}

#visual-portal-wrapper {
    width: 932px;
    margin: 25px auto;
    background: transparent url(&dtml-portal_url;/site_bg.png) repeat-y;
    padding: 0 0 0 64px;
}

#portal-top {
    width: 919px;
    margin-left: 1px;
    *height: 119px; /* absolute header */
}

#portal-header {
    *width: 919px; /* absolute header */
    *position: absolute; /* absolute header for IE <= 7 */
}

#portal-columns {
    width: 911px;
    margin: 3px 8px 0 1px;
}

#portal-column-content {
    padding-left: 12px;
}

#portal-column-content > div:nth-child(2) {
    padding-top: 12px;
}

#portal-column-two {
    *padding-left: 12px;
    padding-top: 22px;
}

#portal-column-two .visualPadding {
    margin: 55px 0 0 15px;
    padding: 0;
}

/* VIEWLETS */

/* personal bar */
#portal-personaltools-wrapper {
    position: absolute;
    *position: relative;
    margin: -20px 0 0 -12px;
    top: 0;
    padding-right: 15px;
    width: 927px;
    text-align: right;
    background: url(&dtml-portal_url;/site_top.png) no-repeat 0 10px;
    height: 25px;
    font-family: 'Trebuchet MS', sans-serif;
}

#portal-personaltools {
    background: transparent;
    margin-left: 0;
}

#portal-personaltools .actionMenuHeader {
    padding: 0;
}

#portal-personaltools .actionMenuHeader a {
    *padding-right: 16px;
    *background: url('&dtml-portal_url;/personalbar_arrow.png') no-repeat 3px right;
}

#header-links a,
#portal-personaltools a {
    color: #ffffff;
}

#header-links a:hover,
#portal-personaltools a:hover {
    color: &dtml-neutralColor;;
}

#portal-personaltools .actionMenuContent a {
    color: &dtml-primaryColor;;
}

#header-links {
    float: right;
}

#header-links li {
    margin-right: 15px;
}

/* social buttons */
#social-buttons {
    position: absolute;
    left: 925px;
    top: 1px;
}

#social-buttons li {
    display: list-item;
    margin-bottom: 5px;
    max-width: 25em;
}

#social-buttons a {
    display: block;
    *width: 18px;
    *height: 18px;
    *background: url(&dtml-portal_url;/social.png) no-repeat left top;
    width: 22px;
    height: 22px;
    text-indent: -9999px;
}

#social-buttons > li > a {
    background-size: 22px 22px;
    background-repeat: no-repeat;
    *border: 1px solid grey;
    border-radius: 5px;
}

/* facebook */
#social-buttons > li.fb > a {
    background-image: url("https://isaw.nyu.edu/icon-facebook.png");
}
/* flickr */
#social-buttons > li.flickr > a {
    background-image: url("https://isaw.nyu.edu/icon-flickr-oldschool.png");
}


/* foursquare */
#social-buttons > li.fsq > a {
    background-image: url("https://isaw.nyu.edu/icon-foursquare.png");
}

/* google plus */
#social-buttons > li.gplus > a {
    background-image: url("https://isaw.nyu.edu/icon-googleplus.png");
}


/* instagram */
#social-buttons > li.instagram > a {
    background-image: url("https://isaw.nyu.edu/icon-instagram.png");
}

/* twitter */
#social-buttons > li.tw > a {
    background-image: url("https://isaw.nyu.edu/icon-twitter.png");
}

/* rss feed */
#social-buttons > li.rss >  a {
    background-image: url("https://isaw.nyu.edu/icon-feed.png");
}

/* mouseover changes */
#social-buttons > li > a:hover {
    *border: 1px solid #6F5C54;
    text-indent: 30px;
    width: auto;
    color: #231F20;
    background-color: white;
    text-decoration: none;
    white-space: nowrap;
    line-height: 22px;
    padding-right: 8px;
    vertical-align: middle;
}

/* search */
#portal-searchbox {
    margin: 5px 0 24px;
    *margin-top: 0;
    *float: none; /* absolute header */
}

.LSBox {
    padding: 0;
    *float: right; /* absolute header */
}

#searchGadget {
    border: 1px solid #a7a4a0;
    background: #eeece7 url(&dtml-portal_url;/search_button.gif) no-repeat top right;
    font-size: &dtml-fontSmallSize;;
    height: 13px;
    line-height: 13px;
    padding-left: 3px; 
}

#portal-searchbox .searchButton {
    border: none;
    background-color: transparent;
    cursor: pointer;
    width: 15px;
    height: 13px;
    margin: 0 0 0 -15px;
    position: absolute;
}

#portal-searchbox .searchSection {
    display: none;
}

#LSResult {
    margin-top: 0;
    *margin-top: 12px;
}

.livesearchContainer {
    padding: 0 0 0 5px;
}

#livesearchLegend {
    margin-top: -12px;
}

#search-filter {
    margin-left: -3em;
    background-color: #fff;
}

/* logo */
#isaw-logo {
    position: absolute;
    margin-left: -65px;
    top: 0px;
}

#portal-logo {
    float: none;
}

#portal-logo img {
    margin: 0 0 30px 8px;
    *margin-top: 17px; /* absolute header */
}

/* breadcrumbs */
#portal-breadcrumbs {
    font-size: &dtml-fontSmallSize;;
    color: #666666;
    line-height: 8px;
    margin: 0 0 15px 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

#portal-breadcrumbs a {
    color: #666666;
    display: inline;
    line-height: 14px;
}

#breadcrumbs-current {
    color: #333333;
}

/* global nav */
#portal-column-nav {
    width: 133px;
    background-color: &dtml-neutralColor;;
}

#portal-globalnav {
    background-color: #cdb08a;
    font-family: "Trebuchet MS", sans-serif;
    padding: 5px 0;
}

#portal-globalnav li {
    display: list-item;
    float: none;
}

#portal-globalnav li a {
    background: transparent;
    color: #ffffff;
    padding: 2px 5px;
    margin-bottom: 5px;
    border: none;
    text-transform: uppercase;
    display: block;
}

#portal-globalnav li a:hover,
#portal-globalnav li.hover a,
#portal-globalnav li.selected.hover a,
#portal-globalnav li.selected a:hover {
    color: &dtml-primaryColor;;
    background-color: #e6ccac;
}

#portal-globalnav li.selected a {
    color: &dtml-primaryColor;;
    background-color: transparent;
}

#portal-column-nav .contact {
    *text-indent: -99999px;
    *background: url(&dtml-portal_url;/contact.gif) no-repeat;
    margin: 15px 0 50px 4px;
    *height: 81px;
    font-size: 13px;
}
    
    /* dropdowns */
    #portal-globalnav ul {
        margin: -45% 0 0 129px !important;
        border: none;
        background-color: #e6ccac;
        /* opacity: 0.9; */
        font-size: 14px;
    }
    
    #portal-globalnav ul.submenu a,
    #portal-globalnav li.selected.hover ul.submenu a {
        border: none;
        text-transform: none;
        color: #716558;
        padding-left: 15px;
    }
    
    #portal-globalnav ul.submenu a:hover,
    #portal-globalnav li.selected ul.submenu a:hover {
        color: &dtml-primaryColor;;
        border: none;
        background-color: transparent;
    }
    
    #portal-globalnav ul.submenu a:hover span {
        text-decoration: underline;
    }
    
/* site actions */
#portal-siteactions {
    position: absolute;
    text-align: right;
    font-size: &dtml-fontSmallSize;;
    background: url(&dtml-portal_url;/site_bottom.png) no-repeat;
    width: 923px;
    padding: 12px 12px 20px 0;
    margin: -5px 0 0 -4px;
}

#portal-siteactions li {
    margin: 0 0 0 25px;
}

#portal-siteactions a {
    color: #ffffff;
}

#portal-siteactions a:hover {
    color: &dtml-neutralColor;;
}

/* share this */
#socialtools {
    font-size: 12px;
    margin-top: -4px;
    padding-left: 135px;
    height: 20px !important;
}

#at15s {
    top: 192px !important;
}

/* PORTLETS */
.portlet {
    background-color: &dtml-neutralColor;;
}

.portlet dt {
    background-color: transparent;
    font-weight: normal;
    color: &dtml-primaryColor;;
    padding: 8px 8px 0;
}

.portlet dt a,
.portlet dt a:visited {
    color: &dtml-primaryColor;;
}

.portlet .portletItem {
    padding: 8px;
    font-size: 13px;
}

.portlet .portletItem a,
.portlet .portletItem a:visited {
    color: #4d4d4d;
}

.portlet .portletItem a:hover {
    text-decoration: underline;
}

.portletFeatured .portletItem img {
    margin-top: -12px;
    margin-bottom: 8px;
}

/* navigation */
.portlet .navTreeLevel0 > .navTreeItem {
    border: none;
    margin-bottom: 10px;
}

.portlet .navTree a {
    padding: 0;
    line-height: 1.2em;
}

/* PAGE HEADING AND RELATED ELEMENTS */
#above-content {
    width: 743px;
    *width: 737px;
    position: absolute;
}

#content,
#edit-bar,
.portalMessage.error,
.portalMessage.warning {
    margin-top: 55px;
}

.template-home_page_view #content,
.template-home_page_view #edit-bar,
.template-home_page_view .portalMessage.error,
.template-home_page_view .portalMessage.warning {
    margin-top: 0 !important;
}

.documentEditable #content {
    margin-top: 14px !important;
}

.documentHeading {
    margin: -10px 0;
    *margin-bottom: -4px;
    *max-width: 565px;
}

.documentHeading span {
    line-height: 26px;
    *line-height: 30px;
    padding-right: 4px;
    background-color: #F3F1EB;
}

#content .documentFirstHeading {
    display: none;
}

/* CONTENT */
#content {
    margin-right: 0px;
    margin-bottom: 24px;
    margin-left: 0;
}

#content .image-caption {
    margin: 0;
    font-size: 12px;
    color: #6e5b53;
    text-align: center;
}

#content .callout {
    background-color: transparent;
    border-right: 3px solid #050505;
    border-left: 3px solid #050505;
    color: #a3462b;
    padding: 6px;
    margin: 0 45px 12px;
    font-size: 13px;
    font-weight: bold;
}

#content .callout a {
    text-decoration: underline;
}

.subheading {
    font-size: 15px;
}

/* EDITING, TINYMCE STYLES */
#text_ifr {
    background-color: #ffffff;
}

.learnMore {
    color: #808080;
    font-size: 12px;
    background: url(&dtml-portal_url;/gray_bullet.png) no-repeat 0 4px;
    padding-left: 8px;
}

.learnMore a {
    color: #808080;
}

.learnMore,
.learnMore a:hover {
    color: &dtml-linkHoverColor;;
}

.template-manage-portlets #above-content {
    width: 310px;
    border: none;
}

.template-manage-portlets #socialtools {
    display: none;
}

/* HOMEPAGE */
.template-home_page_view #portal-column-content {
    padding-left: 12px;
}

.template-home_page_view #above-content {
    display: none;
}

.template-home_page_view #content {
    background: transparent none;
    line-height: 20px;
    margin-top: 0 !important;
    margin-right: 3px;
}

#home-left {
    float: left;
    width: 176px;
}

#home-left p {
    margin-right: 0px;
}
#home-center {
    width: 565px;
    margin-right: 5px;
    float: right;
}

#home-center p {
    margin-right: 0px;
}

#home-center h4 a {
    font-weight: normal;
}

#home-events,
#home-news {
    width: 340px;
    height: 390px;
    padding-top: 21px;
}

#home-news {
    font-size: 14px;
    clear: left;
}

#home-events h2,
#home-news h2 {
    
}

#home-events h3,
#home-news h3 {
    font-size: 15px;
    line-height: 1.2em;
    font-weight: 500;
}

.homeNews,
.homeEvent {
    font-size: 14px;
    margin-top: 14px;
}
.homeEvent {
    width: 340px;
    padding-right: 40px;
    float: left;
}

.homeEvent p, 
.homeNews p {
    font-size: 14px;
    margin-top: 0px !important;
    margin-bottom: 0px !important;
}

.homeEvent .speaker {
    font-weight: 600;
}

.homeEvent .eventDate, .homeEvent .eventTime {
    font-weight: 400;
    color: #666666;
    font-size: 11px !important;
}

.homeEvent .eventLocation {
    font-style: italic;
}


.home-right {
    float: right;
    width: 165px;
    margin-top: 25px;
    padding-left: 5px;
    min-height: 107px;
    background: Text no-repeat top left;
    text-align: right;
    font-size: 14px;
    color: #6d6e70;
}

.annotation {
    writing-mode:tb-rl;
    -webkit-transform:rotate(270deg);
    -moz-transform:rotate(270deg);
    -o-transform: rotate(270deg);
    white-space:nowrap;
    display:block;
    font-family:'Arial', Helvetica, sans-serif;
    font-size:20px;
    font-weight:normal;
    position: relative;
    top: 80px;
    left: -100px;
    float: left;
    color: #CDB08A;
}

.featuredDate {
    font-size: 12px;
    font-style: italic;
}

#content .featuredDescrip {
    margin: 0;
}

.featuredLink {
    color: #808080;
    font-size: &dtml-fontSmallSize;;
    font-weight: bold;
    text-align: right;
    display: block;
    padding-right: 15px;
}

.ceTitle {
    color: #42250d;
    font-size: 14px;
}

.ceDescrip {
    color: #404041;
}

.template-news_view div#content-core > div:first-child {
    margin-bottom: 2em;
    line-height: 1.1em !important;
} 

.template-news_view .documentDescription {
    font-size: 15px;
    color: black;
    font-style: italic;
    margin-bottom: 1em;
    margin-top: 1em;
}

.isawNewsSummary {
    margin-bottom: 2em;
}

.isawNewsSummary h2.tileHeadline {
    font-weight: normal;
    line-height: 1.1em;
    margin-right: 15px;
}

.isawNewsSummary .newsTileImage {
    float: left !important;
    margin: 5px 1em 0 0 !important;
    border-right: 1px solid #CCCCCC;
    border-bottom: 1px solid #CCCCCC;
    border-left: 1px solid #EEEEEE;
    border-top: 1px solid #EEEEEE;
}

.isawNewsSummary .eventSpeaker {
    color: inherit !important;
    font-size: inherit !important: 
}

.isawNewsSummary .documentByLine {
    margin-top: -2px;
}

.template-newsitem_view .documentByLine {
    margin-bottom: 1.5em;
}

img.newsImage {
    width: 200px;
    height: inherit;
    border-right: 1px solid #CCCCCC !important;
    border-bottom: 1px solid #CCCCCC !important;
    border-left: 1px solid #EEEEEE !important;
    border-top: 1px solid #EEEEEE !important;}

