<?xml version="1.0"?>
<object name="portal_javascripts">
    <!-- TinyMCE redux -->
    <javascript compression="safe"
      authenticated="True"
      id="jquery.tinymce.js"
      insert-after="unlockOnFormUnload.js"
      />
    <javascript compression="safe"
      authenticated="True"
      id="tiny_mce_gzip.js"
      insert-after="jquery.tinymce.js"
      />

    <javascript id="++resource++plone.app.jquerytools.plugins.js" enabled="True"/>

    <javascript cacheable="True" compression="none" cookable="True"
                enabled="True" inline="False"
                id="++theme++isaw.theme/js/isaw.js"
                insert-after="jquery-integration.js"/>

    <javascript cacheable="True" compression="none" cookable="False"
                enabled="True" inline="False"
                id="++theme++isaw.theme/js/awld/lib/requirejs/require.min.js"
                insert-after="*"/>

    <javascript cacheable="True" compression="none" cookable="False"
                enabled="True" inline="False"
                id="++theme++isaw.theme/js/awld/awld.js"
                insert-after="++theme++isaw.theme/awld/lib/requirejs/require.min.js"/>

    <javascript cacheable="True" compression="none" cookable="True"
                enabled="True" inline="False"
                id="++resource++plone.formwidget.geolocation/libs.js"
                insert-before="++theme++isaw.theme/js/isaw.js"/>


    <!-- remove obsolete js -->
    <javascript id="isaw.js" remove="True"/>
    <javascript id="jquery.tools.min.js" remove="True"/>
    <javascript id="tiny_mce_init.js" remove="True" />
    <javascript id="tiny_mce_src.js" remove="True" />
    <javascript id="++theme++isaw.theme/js/jquery.cycle2.min.js" remove="True" />
</object>
